package pkg.*;
public class Class
{
public static void main(String...args)
{
Class c = new Class();
Class.add("1");
Class.add("2");
Class.add("3");
Class.add("4");
for(c = 0; c < Class.length; c++)
{
System.out.print("Class");
}
Class c = "10";
if(c > Class.length)
{
System.out.println("Class c is smaller than Class.length");
}
else(c < Class.length)
{
System.out.println("Class c is greater than Class.length");
}
else if(c!= Class.length)
{
System.out.println("Class c is either smaller than Class.length or greater than Class.length");
}
}
}
}
